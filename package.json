{
  "name": "scroll-triggers",
  "description": "A tiny, dependency-less javascript library to add classes as elements scroll into viewport.",
  "homepage": "https://github.com/firstandthird/scroll-triggers",
  "author": "First+Third",
  "license": "MIT",
  "version": "4.1.0",
  "main": "index.js",
  "module": "dist/scroll-triggers.js",
  "scripts": {
    "start": "scriptkit",
    "build": "scriptkit build",
    "dev": "scriptkit dev",
    "prepublishOnly": "scriptkit"
  },
  "scriptkit": {
    "files": {
      "dist/scroll-triggers.js": "index.js"
    },
    "test": {
      "enabled": false
    }
  },
  "keywords": [
    "browser",
    "scroll",
    "declarative"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/firstandthird/scroll-triggers.git"
  },
  "dependencies": {
    "attrobj": "^3.1.0",
    "domassist": "^2.2.0",
    "tinybounce": "^1.2.0"
  },
  "devDependencies": {
    "eslint-config-firstandthird": "^4.4.0",
    "eslint-plugin-import": "^2.18.0",
    "scriptkit": "^3.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": "firstandthird"
  }
}
